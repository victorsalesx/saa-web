<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="       http://java.sun.com/xml/ns/javaee       http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
	<!-- Login Inicio -->
	<navigation-rule>
		<from-view-id>/pages/login/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>cadastro</from-outcome>
			<to-view-id>/pages/usuario/cadUsuario.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>atualiza</from-outcome>
			<to-view-id>/pages/usuario/atualizarUsuario.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>homeProfessor</from-outcome>
			<to-view-id>/pages/turma/listaTurma.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>homeAluno</from-outcome>
			<to-view-id>/pages/home/dashboardAtendente.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>homeAdministrador</from-outcome>
			<to-view-id>/pages/home/dashboardAtendente.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>fracasso</from-outcome>
			<to-view-id>/pages/login/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<!-- Login Fim -->
	
	<!-- atualizaSenha -->
	<navigation-rule>
		<from-view-id>/pages/usuario/atualizarUsuario.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>fracasso</from-outcome>
			<to-view-id>/pages/usuario/atualizarUsuario.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>homeProfessor</from-outcome>
			<to-view-id>/pages/home/dashboardAtendente.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>homeAluno</from-outcome>
			<to-view-id>/pages/home/dashboardAtendente.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<!-- fim do atualizaSenha -->
	
	<!-- cadUsuario -->
	<navigation-rule>
		<from-view-id>/pages/usuario/cadUsuario.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>fracasso</from-outcome>
			<to-view-id>/pages/usuario/cadUsuario.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/pages/login/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- Menu inicio -->
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>atendimento</from-outcome>
			<to-view-id>/pages/login/login.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>acompanhamento</from-outcome>
			<to-view-id>/pages/login/login.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>agenda</from-outcome>
			<to-view-id>/pages/login/login.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>funcionarios</from-outcome>
			<to-view-id>/pages/login/login.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>sair</from-outcome>
			<to-view-id>/pages/login/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<!-- Menu Final Converters -->
	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
		<locale-config>
			<supported-locale>pt_BR</supported-locale>
			<supported-locale>en_US</supported-locale>
		</locale-config>
		<resource-bundle>
			<base-name>messages</base-name>
			<var>messages</var>
		</resource-bundle>
	</application>
</faces-config>
